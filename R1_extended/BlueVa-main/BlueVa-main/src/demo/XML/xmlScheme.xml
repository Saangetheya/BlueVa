<?xml-model href="xmlScheme.xsd"?>
<package name="">
    <class name="" extends="" implements="" accmod="" naccmod="">
        <field name="" type="" value="" accmod="" naccmod=""/>
        <method name="" input="" output="" id="" accmod="" naccmod=""/>
    </class>
    <interface name="" implements="" accmod="">
        <method name="" input="" output="" id="" accmod="" naccmod=""/>
    </interface>
    <enums name="" accmod="">
        <value name="" value=""/>
    </enums>
</package>

    <!-- type = classType | enumType | interfaceType -->
    <!-- 
    class
    name          : String [a-zA-Z_]+
    extends       : classType  [a-zA-Z_]+
    implements    : [interfaceType] [[a-zA-Z_]+]s
    accmod        : [public private protected none]
    naccmod       : [abstract final]
     -->
    <!-- 
    field
    name          : String [a-zA-Z_]+
    type          : [int long float double char string byte boolean short]
    value         : [a-zA-Z0-9] + [True/False]
    accmod        : [public private protected none]
    naccmod       : [volatile static transient final none]
    -->
    <!-- 
    method   
    name         : String [a-zA-Z_]+ 
    input        : [[[int long float double char string byte boolean short],nameOfParameter]]
    output       : [int long float double char string byte boolean short]
    id           : UUID
    accmod       : [public private protected none]
    naccmod      : [synchronized static final abstract transient none]
    --> 
    <!--
    interface
    name        : String [a-zA-Z_]+ 
    implements  : [interfaceType]
    accmod      : [public private protected none]
    -->
    <!-- 
    enums
    name         :  String [a-zA-Z_]+
    accmod       :  [public private protected none]
    -->
    <!-- 
     value
     name : String [a-zA-Z_]+
     value : int
    -->